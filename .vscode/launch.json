{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "coreclr",
      "request": "launch",
      "name": "Launch VectorSearch.Api",
      "program": "${workspaceFolder}/src/VectorSearch.Api/bin/Debug/net8.0/VectorSearch.Api.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/VectorSearch.Api",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "preLaunchTask": "build VectorSearch.Api"
    }
  ],
  "inputs": []
}